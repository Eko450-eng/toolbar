{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"formatWithErrors": true,
		"enabled": true,
		"attributePosition": "auto",
		"indentWidth": 2,
		"lineWidth": 80,
		"lineEnding": "lf",
		"indentStyle": "tab"
	},
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": false,
		"rules": {
			"recommended": false
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single"
		}
	}
}
